{
  "manifest_version": 3,
  "name": "Resume Job Finder",
  "version": "1.0",
  "description": "An extension to read a resume (PDF) and find relevant jobs.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}